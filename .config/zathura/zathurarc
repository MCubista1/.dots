set recolor-darkcolor "#ABB2BF"
set default-bg "#263238"

set inputbar-bg "#263238"
set inputbar-fg "#ABB2BF"
set notification-bg "#263238"
set notification-fg "#ABB2BF"
set notification-error-bg "#C3E88D"
set notification-error-fg "#ABB2BF"
set notification-warning-bg "#C3E88D"
set notification-warning-fg "#ABB2BF"
set statusbar-bg "#263238"
set statusbar-fg "#ABB2BF"
set index-bg "#263238"
set index-fg "#ABB2BF"
set index-active-bg "#ABB2BF"
set index-active-fg "#263238"
set render-loading-bg "#263238"
set render-loading-fg "#ABB2BF"

set window-title-home-tilde true
set statusbar-basename true
set selection-clipboard clipboard

map d scroll half-down
map e scroll half-up
map D scroll full-down
map E scroll full-up

map = zoom in
map z zoom default
map x toggle_page_mode

map i recolor
map <C-o> exec "terminator -e term_fff" 

unmap <C-j>
unmap <C-k>
