[include]
	path = ~/.user.gitconfig
[alias]
	# Basic git
	s = status
	d = diff
	a = add
	aa = add .
	c = commit -m
	ca = commit -am
	p = push

	# Pull file from master branch
	ftm = fetch origin master:master
	chm = checkout master

	# Pull file from pi branch
	ftp = fetch origin pi:pi
	chp = checkout pi

	# Log
	l = log --oneline
	lb = log --oneline --graph --branches -n 5

	# List tracked files
	ls = ls-tree -r --name-only HEAD
